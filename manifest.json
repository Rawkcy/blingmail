{
  "manifest_version": 2,

  "name": "EmailWing",
  "description": "This extension makes it easy to submit your emails for approval from others in GMail",
  "version": "0.1",

  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "css": ["emailwing.css"],
      "js": [
        "jquery-1.8.3.min.js", 
        "firebase.js",
        "emailwing.js"
      ],
      "run_at": "document_end"
    }
  ]
}
